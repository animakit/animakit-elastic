{
  "name": "animakit-elastic",
  "version": "1.0.0",
  "description": "AnimakitElastic - React component for flexible resizing of the blocks",
  "main": "index.js",
  "files": [
    "*.md",
    "lib/*.js"
  ],
  "scripts": {
    "clean": "rimraf lib/*.js",
    "build": "npm run clean && babel -s inline -d ./ **/*.es6",
    "prepublish": "npm run build",
    "test": "$(npm bin)/jest"
  },
  "author": "Anna Selezniova",
  "license": "MIT",
  "keywords": [
    "animakit",
    "animation",
    "elastic",
    "flexible",
    "resize",
    "react",
    "react-component"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/animakit/animakit-elastic"
  },
  "homepage": "https://animakit.github.io",
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-3": "^6.22.0",
    "enzyme": "^2.7.1",
    "enzyme-to-json": "^1.5.0",
    "jest": "^19.0.2",
    "jsdom": "^9.11.0",
    "react-addons-test-utils": "^15.4.2",
    "rimraf": "^2.6.1"
  },
  "dependencies": {
    "lodash.isequal": "^4.5.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFiles": [
      "<rootDir>/test/setup.js"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  }
}
